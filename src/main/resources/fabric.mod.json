{
  "schemaVersion": 1,
  "id": "team-manager",
  "version": "${version}",

  "name": "Team Manager",
  "description": "A minecraft mod to add player and admin team management to minecraft servers.",
  "authors": [
    "Synthesyzer"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "CC0-1.0",
  "icon": "assets/teammanager/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.synthesyzer.teammanager.TeamManager"
    ],
    "client": [
      "com.synthesyzer.teammanager.client.TeamManagerClient"
    ]
  },
  "mixins": [
    "modid.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.19",
    "minecraft": "~1.19.2",
    "java": ">=17",
    "fabric-api": "*"
  },
  "suggests": {
    "another-mod": "*"
  }
}
